{
  "name": "dadscape-diary-api",
  "version": "1.0.0",
  "description": "Backend API for DadScape clan achievement diaries",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "ts-node src/db/migrate.ts",
    "seed": "ts-node src/db/seed.ts",
    "seed-test-data": "npm run migrate && npm run seed",
    "test": "jest"
  },
  "keywords": [
    "runelite",
    "osrs",
    "clan",
    "achievement",
    "diary"
  ],
  "author": "DadScape",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.8",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.3",
    "@types/sqlite3": "^3.1.11"
  }
}
